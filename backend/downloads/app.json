{
  "expo": {
    "name": "Kwezi",
    "slug": "kwezi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kwezi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "Apprends le Shimaor√© et le Kibouchi, les langues authentiques de Mayotte",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mahoraiseducation.kwezi"
    },
    "android": {
      "package": "com.mahoraiseducation.kwezi",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "corsOrigins": [
        "https://app.emergent.sh",
        "https://emergent.sh",
        "https://mahorais-learn.preview.emergentagent.com"
      ]
    }
  }
}
