{
  "expo": {
    "name": "Kwezi",
    "slug": "kwezi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "kwezi",
    "userInterfaceStyle": "automatic",
    "description": "Apprends le Shimaor√© et le Kibouchi, les langues authentiques de Mayotte",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mahoraiseducation.kwezi"
    },
    "android": {
      "package": "com.mahoraiseducation.kwezi",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "enableProguardInReleaseBuilds": false,
            "enableShrinkResourcesInReleaseBuilds": false
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "cli": {
      "appVersionSource": "remote",
      "requireCommit": false
    },
    "extra": {
      "backendUrl": "https://kwezi-backend.onrender.com",
      "corsOrigins": [
        "https://app.emergent.sh",
        "https://emergent.sh",
        "https://kwezi-android.preview.emergentagent.com"
      ],
      "eas": {
        "projectId": "308793ab-c80f-403a-a8b3-7cbfac1b57c6"
      }
    }
  }
}
